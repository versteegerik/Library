{
  "compilerOptions": {
    "outDir": "./wwwroot/build/",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "module": "commonjs",
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "moment": [
        "node_modules/moment/moment"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "externals": {
    "@versteey/myfirstnpmpackage": "commonjs @versteey/myfirstnpmpackage"
  }
}