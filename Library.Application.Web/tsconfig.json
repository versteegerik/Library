{
  "compilerOptions": {
    "outDir": "./wwwroot/build/",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "module": "es6",
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "jquery-ui": [
        "node_modules/@types/jqueryui/index"
      ],
      "moment": [
        "node_modules/moment/moment"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}